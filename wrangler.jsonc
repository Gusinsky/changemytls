{
  "name": "hoztnamer",
  "main": "src/index.ts",
  "compatibility_date": "2025-03-07",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "YOUR_DBNAME",
      "database_id": "YOUR_DBID"
    }
  ],
  "vars": {
    "ENABLE_EXPORT": "true",
    "ENABLE_UPDATE": "true",
    "CF_API_TOKEN": "YOUR_CF_API_TOKEN",
    "ZONE_ID": "YOUR ZONE_ID"
  }
}
